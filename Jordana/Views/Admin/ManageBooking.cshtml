@{
	Layout = "_LayoutAdmin";
}
<main class="content">
	<div class="container-fluid p-0">

		<h1 class="h3 mb-3"><strong>Analytics</strong> Dashboard</h1>

		<div class="row">

			<div class="col-18 col-lg-16 col-xxl-12 d-flex">
				<div class="card flex-fill">
					<div class="card-header">

						<h5 class="card-title mb-0">Latest Booking</h5>
					</div>
					<table class="table table-hover my-0">
						<thead>
							<tr>
								<th>Tourist_Sites</th>
								<th class="d-none d-xl-table-cell">Start Date</th>
								<th class="d-none d-xl-table-cell">Location</th>
								<th>Invited Members</th>
								<th class="d-none d-md-table-cell">Total Price</th>
								<th class="d-none d-md-table-cell">Action</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<td>The Roman Theater</td>
								<td class="d-none d-xl-table-cell">01/01/2021</td>
								<td class="d-none d-xl-table-cell">Amman</td>
								<td><span class="badge bg-success">7</span></td>
								<td class="d-none d-md-table-cell">800 </td>
								<td>
									<a class="btn btn-outline-success" style="float:left; margin-left:12px;" >Approve</a>
									<a class="btn btn-outline-info" style="margin-left:12px;" asp-action="GetBookingDetails">Get details</a>
									<a class="btn btn-outline-danger" style="margin-left:12px;" >Reject</a>
								</td>
							</tr>
							
							
							
							
						

							




						</tbody>
					</table>
				</div>
			</div>

		</div>

	</div>
</main>